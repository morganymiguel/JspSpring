package kr.or.ddit.servlet02;

import java.io.IOException;

import javax.servlet.ServletException;
import javax.servlet.annotation.WebServlet;
import javax.servlet.http.HttpServlet;
import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;

@WebServlet("/gugudan.do")
public class GugudanServlet extends HttpServlet {
	@Override
	protected void doGet(HttpServletRequest req, HttpServletResponse resp) throws ServletException, IOException {
		StringBuffer gugudan = new StringBuffer();
		StringBuffer options = new StringBuffer();
		
		String
		

		
		
		
		
		
		
		
		for(int i =2; i<10;i++) {
			gugudan.append("<tr>");
			for(int j = 1; j<10; j++) {
				gugudan.append(String.format("<td>%d * %d = %d</td>", i, j, i*j));
			}
			gugudan.append("</tr>");
		}
		req.setAttribute("gugudan", gugudan);
		req.getRequestDispatcher("/WEB-INF/views/tmpl/gugudan.tmpl").forward(req,resp);
		
	}
}
