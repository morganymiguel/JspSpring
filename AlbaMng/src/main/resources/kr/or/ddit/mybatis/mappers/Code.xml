<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.ddit.alba.dao.CodeDAO">
	<select id="selectLicenseList" resultType="LicenseVO">
		SELECT LIC_CODE, LIC_NAME
		FROM LICENSE
	</select>
	<select id="selectGradeList" resultType="hashMap">
		SELECT GR_CODE "grCode", GR_NAME "grName"
		FROM GRADE
	</select>
</mapper>